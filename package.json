{
	"name": "andromeda-nails",
	"type": "module",
	"version": "1.1.0",
	"description": "Explora nuestra colección de diseños de uñas acrílicas, techgel y comerciales. Encuentra inspiración en nuestra variada galería de imágenes de manicuras y tendencias de belleza.",
	"author": "Juan Fontalvo (softDev) - Web Develoment",
	"license": "GPL-3.0-only",
	"repository": {
		"type": "git",
		"url": "https://github.com/softDev-os/andromeda-nails"
	},
	"keywords": [
		"astro",
		"astrojs",
		"astro-andromeda-nails"
	],
	"bugs": {
		"url": "https://github.com/softDev-os/andromeda-nails/issues"
	},
	"scripts": {
		"dev": "tinacms dev -c 'astro dev'",
		"start": "astro dev",
		"build": "astro build",
		"sync": "astro sync",
		"preview": "astro preview",
		"postbuild": "pagefind --site dist",
		"format:check": "prettier --plugin-search-dir=. --check .",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"pre-commit": "lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"@astrojs/rss": "3.0.0",
		"@tinacms/cli": "^1.5.39",
		"astro": "3.3.1",
		"better-sqlite3": "^11.6.0",
		"sharp": "^0.33.5",
		"tinacms": "^1.5.28"
	},
	"devDependencies": {
		"@astrojs/mdx": "1.1.1",
		"@astrojs/sitemap": "3.0.1",
		"@astrojs/tailwind": "5.0.1",
		"@pagefind/default-ui": "^1.0.4",
		"@tailwindcss/typography": "0.5.10",
		"@typescript-eslint/parser": "^6.18.1",
		"clsx": "2.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-astro": "^0.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"mdast-util-to-string": "^4.0.0",
		"motion": "^10.17.0",
		"pagefind": "^1.0.4",
		"prettier": "^3.1.1",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.12.3",
		"reading-time": "^1.5.0",
		"slugify": "^1.6.6",
		"tailwind-merge": "1.14.0",
		"tailwindcss": "3.3.3",
		"typescript": "^5.3.3"
	},
	"lint-staged": {
		"*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
			"prettier --write --plugin-search-dir=."
		]
	}
}
